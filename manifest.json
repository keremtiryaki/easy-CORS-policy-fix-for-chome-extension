{
    "manifest_version": 2,
    "version": "1.0.0",
    "name": "easy-CORS-policy-fix-for-chome-extension",
    "short_name": "easy-CORS-policy-fix-for-chome-extension",
    "description": "",
    "icons": {
      "16": "assets/img/icon.png",
      "32": "assets/img/icon.png",
      "128": "assets/img/icon.png"
    },
    "browser_action": {
      "default_title": "easy-CORS-policy-fix-for-chome-extension"
    },
    "background": {
      "scripts": [
        "bg.js"
      ],
      "persistent": true
    },
    "content_scripts": [
      {
        "js": [
          "content.js"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "permissions": [
      "<all_urls>"
    ]
  }